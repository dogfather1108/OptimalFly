/* pages/index/login/index.wxss */
page{
  width: 100%;
  height: 100%;
}
.contain{
  width: 100%;
  height: 100%;
  background-size: cover;
  display: flex;
  flex-direction: column;
  align-items: center
}
.contain>image{
  width: 194rpx;
  height: 87rpx;
  margin-top: 144rpx;
}
.section{
  margin-top: 233rpx;
  width: 600rpx;
}
.section>view{
  width: 100%;
  display: flex;
  flex-direction: row;
  align-items: center;
  background: white;
  height: 96rpx;
  border-radius: 48rpx;
}
.section>view label:nth-of-type(1){
  width: 73rpx;
  height: 73rpx;
  border-radius: 100%;
  background: rgba(43,209,255,1);
  margin-left: 11rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
}
.section>view input{
  margin-left: 44rpx;
  font-size: 30rpx;
  color: rgba(51,51,51,1);
  font-weight:bold;
}
.section>view:nth-of-type(1) input{
  width: 300rpx;
}
.section>view:nth-of-type(2){
  margin-top: 19rpx;
}
.section>view:nth-of-type(2) input{
  width: 200rpx;
}
.section>view:nth-of-type(2) label:nth-of-type(2){
  font-size: 30rpx;
  color:rgba(43,209,255,1);
  width: 150rpx;
  text-align: center;
  margin-left: 80rpx;
}
button{
  height: 96rpx;
  box-sizing: border-box;
  border-radius: 48rpx;
  background: transparent;
  margin-top: 82rpx;
  border:2rpx solid rgba(43,209,255,1);
  color:rgba(43,209,255,1); 
}