page{
  height: 100%;
}
.contain{
  height: 100%;
  background: #eee;
}
scroll-view{
  height: 100%;
}
.item{
  width: 100%;
  height: 175rpx;
  background: white;
  box-sizing: border-box;
  padding: 37rpx 24rpx;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
}
.item>text{
  font-size:30rpx;
  font-family:PingFang-SC-Regular;
  font-weight:400;
  color:rgba(51,51,51,1);
}
.item:nth-of-type(1){
  margin-top: 20rpx;
}
.item:nth-of-type(n+2){
  margin-top: 2rpx;
}
.item>view{
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-between;
  width: 100%;
}
.item>view>text{
  font-size:28rpx;
  font-family:PingFang-SC-Regular;
  font-weight:400;
  color:rgba(51,51,51,1);
}
.item>view>view{
  display: flex;
  flex-direction: row;
  font-size:28rpx;
  font-family:PingFang-SC-Bold;
  font-weight:bold;
  color:rgba(51,51,51,1);
}
.item>view>view label{
  align-self: flex-end;
  font-size:24rpx;
  font-family:PingFang-SC-Bold;
  font-weight:bold;
  color:rgba(51,51,51,1);
}