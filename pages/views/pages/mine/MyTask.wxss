page{
  width: 100%;
  height: 100%;
}
.contain{
  height: 100%;
  background: #eee;
  display: flex;
  flex-direction: column;
}
.tarbar{
  width: 100%;
  height: 100rpx;
  display: flex;
  flex-direction: row;
  justify-content: space-around;
  margin-top: 2rpx;
  background: white;
}
.tarbar label{
  font-size:28rpx;
  font-family:PingFang-SC-Medium;
  font-weight:500;
  color:rgba(0,0,0,1);
  line-height:100rpx;
  height: 100rpx;
  box-sizing: border-box;
}
scroll-view{
  flex: 1;
  width: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  box-sizing: border-box;
}
.item{
  background: white;
  width: 702rpx;
  position: relative;
  margin: auto;
  border-radius: 20rpx;
  padding: 33rpx 0;
  box-sizing: border-box;
  margin-top: 27rpx;
}
.item:nth-last-of-type(1){
  margin-bottom: 27rpx;
}
.item-t{
  display: flex;
  flex-direction: row;
  padding: 0 44rpx;
  box-sizing: border-box;
  width: 100%;
  border-bottom: 2rpx solid rgba(236,236,236,1);
  padding-bottom: 38rpx;
}
.item-t>view{
  flex: 1;
  box-sizing: border-box;
  padding-left: 35rpx;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
}
.item-t>view>view:nth-of-type(1){
  font-size:32rpx;
  font-family:PingFang-SC-Bold;
  color:rgba(51,51,51,1);
  display: flex;
  flex-direction: column;
}
.item-t>view>view:nth-of-type(1) text:nth-of-type(1){
  font-weight:bold;
}
.item-t>view>view:nth-of-type(1) text:nth-of-type(2){
  font-size:26rpx;
  font-family:PingFang-SC-Regular;
  font-weight:400;
  color:rgba(153,153,153,1);
  margin-top: 10rpx;
}
.item-t>view>view:nth-of-type(2){
  display: flex;
  flex-direction: row;
  width: 100%;
  justify-content: space-between;
}
.item-t>view>view:nth-of-type(2)>text{
  font-size:28rpx;
  font-family:PingFang-SC-Regular;
  font-weight:400;
  color:rgba(51,51,51,1);
}
.item-t>view>view:nth-of-type(2) view{
  display: flex;
  flex-direction: row;
}
.item-t>view>view:nth-of-type(2) view image{
  align-self: flex-end;
}
.item-t>view>view:nth-of-type(2) view label{
  align-self: flex-end;
  font-size:24rpx;
  font-family:PingFang-SC-Bold;
  color:rgba(83,212,205,1);
}
.item-t>view>view:nth-of-type(2) view text{
  font-size:28rpx;
  font-family:PingFang-SC-Bold;
  font-weight:bold;
  color:rgba(83,212,205,1);
  align-self: flex-end;
  line-height: 30rpx;
}
.item-m{
  padding-top: 27rpx;
  padding-left: 44rpx;
  padding-right: 44rpx;
  width: 100%;
  box-sizing: border-box;
}
.item-m view{
  display: flex;
  flex-direction: row;
  align-items: center;
}
.item-m view text:nth-of-type(1){
  font-size:28rpx;
  font-family:PingFang-SC-Regular;
  font-weight:400;
  color:rgba(51,51,51,1);
  margin-left: 16rpx;
}
.item-m view text:nth-of-type(2){
  font-size:28rpx;
  font-family:PingFang-SC-Regular;
  font-weight:bold;
  color:rgba(51,51,51,1);
  margin-left: 21rpx;
  width: 400rpx;
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
}