page{
  height: 100%;
  width: 100%;
}
.contain{
  width: 100%;
  height: 100%;
  display: flex;
  flex-direction: column;
  background: #eee;
}
.tarbar{
  width: 100%;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-around;
  height: 100rpx;
  margin-top: 1rpx;
  background: white;
}
.tarbar label{
  width: 66rpx;
  color:rgba(43,209,255,1);
  font-size:28rpx;
  height: 100rpx;
  line-height: 100rpx;
  text-align: center;
  box-sizing: border-box;
}
.borderColor{
  border-bottom: 8rpx solid rgba(43,209,255,1);
}
scroll-view{
  flex: 1;
  display: flex;
  flex-direction: column;
}
.item{
  width: 100%;
  display: flex;
  flex-direction: row;
  padding: 25rpx 26rpx;
  box-sizing: border-box;
  background: white;
  margin-top: 11rpx;
}
.item>view{
  display: flex;
  flex-direction: column;
  height: 150rpx;
  margin-left: 26rpx;
  justify-content: space-between;
}
.item>view>view:nth-of-type(1) text:nth-of-type(1){
  font-size:32rpx;
  font-weight:bold;
  color:rgba(51,51,51,1);
  display: block;
}
.item>view>view:nth-of-type(1) text:nth-of-type(2){
  display: block;
  width:468rpx;
  font-size:24rpx;
  font-weight:400;
  color:rgba(153,153,153,1);
  margin-top: 17rpx;
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
}
.item>view>view:nth-of-type(2){
  width: 480rpx;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
}
.item>view>view:nth-of-type(2) view{
  display: flex;
  flex-direction: row;
}
.item>view>view:nth-of-type(2) view:nth-of-type(1) view{
  font-size:30rpx;
  font-weight:bold;
  display: block;
  color:rgba(153,153,153,1);
  align-self: flex-end;
  margin-right: 5rpx;
  line-height: 35rpx;
}
.item>view>view:nth-of-type(2) view:nth-of-type(1) label:nth-of-type(1){
  font-size: 25rpx;
  align-self: flex-end;
  color:rgba(153,153,153,1);
}
.item>view>view:nth-of-type(2) view:nth-of-type(1) label:nth-of-type(2){
  align-self: flex-end;
  width: 64rpx;
  height: 24rpx;
  border-radius:6rpx;
  background:rgba(153,153,153,1);
  color: white;
  font-weight: 400;
  font-size:18rpx;
  text-align: center;
  line-height: 20rpx;
  margin-bottom: 5rpx;
}
.item>view>view:nth-of-type(2) view:nth-of-type(2){
  display: flex;
  flex-direction: row;
}
.item>view>view:nth-of-type(2) view:nth-of-type(2) view{
  font-size:30rpx;
  font-weight:bold;
  display: block;
  color:rgba(43,209,255,1);
  align-self: flex-end;
  margin-right: 5rpx;
  line-height: 35rpx;
}
.item>view>view:nth-of-type(2) view:nth-of-type(2) label{
  font-size: 25rpx;
  align-self: flex-end;
  color:rgba(43,209,255,1);
}
.item>view>view:nth-of-type(2) view:nth-of-type(2) image{
  margin-bottom: 5rpx;
}